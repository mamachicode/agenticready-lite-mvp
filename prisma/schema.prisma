generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Order {
  websiteUrl String?
  id              String   @id @default(cuid())
  email           String
  status          String
  stripeSessionId String   @unique
  amount          Int
  currency        String
  reportS3Key     String?
  fulfilledAt     DateTime?
  createdAt       DateTime @default(now())
}
